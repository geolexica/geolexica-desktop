{
  "name": "isotc211-geolexica-desktop",
  "version": "0.0.7",
  "description": "Cross-platform graphical terminal for collaborative editing of ISO/TC 211 Geolexica terminology database",
  "main": "index.js",
  "repository": "git@github.com:ISO-TC211/geolexica-desktop.git",
  "license": "MIT",
  "electronWebpack": {
    "staticSourceDirectory": "src/static",
    "main": {
      "webpackConfig": "main.webpack.js"
    },
    "renderer": {
      "webpackConfig": "renderer.webpack.js"
    }
  },
  "build": {
    "productName": "Geolexica Desktop"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder -ml",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/throttle-debounce": "^2.1.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^0.2.1",
    "electron-react-devtools": "^0.5.3",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "typescript": "^3.7.2",
    "webpack": "~4.35.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/icons": "^3.12.0",
    "@blueprintjs/select": "^3.11.2",
    "@types/node-fetch": "^2.5.4",
    "async-lock": "^1.2.2",
    "electron-log": "^3.0.9",
    "fs-extra": "^8.1.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "source-map-support": "^0.5.16",
    "sse": "https://github.com/riboseinc/sse-elements#sse-elements-v0.3.0-beta2-gitpkg",
    "throttle-debounce": "^2.1.0"
  },
  "resolutions": {
    "lodash": "4.17.13",
    "set-value": "2.0.1",
    "mixin-deep": "1.3.2",
    "js-yaml": "3.13.1"
  }
}
